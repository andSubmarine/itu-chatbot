{
    "name": "niclashedam/itu-chatbot",
    "description": "ITU-Chatbot",
    "keywords": [],
    "license": "Apache 2.0",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "laravel/tinker": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "johngrogg/ics-parser": "^2.1",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9",
        "filp/whoops": "^2.1",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "optimize-autoloader": true
}
